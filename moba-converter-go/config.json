      "options": {
        "Interactive Shell": "0",
        "LXDE": "1"
      }
    },
    "PrivateKeyPath": {
      "default_value": "",
      "section": "Advanced SSH settings",
      "help": "Path to the private key file."
    },
    "SSHGWPrivateKeyList": {
      "default_value": "",
      "section": "Network settings",
      "help": "List of private keys for SSH gateways. Multiple keys are separated by '__PIPE__'."
    },
    "SSHBrowserType": {
      "default_value": "true",
      "section": "Advanced SSH settings",
      "help": "Enable SSH File-browser.",
      "options": {
        "Enable": "-1",
        "Disable": "0",
        "true": "-1",
        "false": "0"
      }
    },
    "FollowSSHPath": {
