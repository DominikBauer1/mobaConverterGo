{
  "_meta": {
    "version": "1.0.0",
    "changed_when": "2024-06-25"
  },
  "session_types": {
    "ssh": {
      "session_type": "109",
      "tmplString": "{{.sessionName}}={{ .Logout }}#{{ .IconNumber }}#{{ .SessionType }}%{{ .RemoteHost }}%{{ .Port }}%{{ .Username }}%%{{ .X11Forwarding }}%{{ .Compression }}%{{ .ExecuteCommand }}%{{ .SSHGWHostList }}%{{ .SSHGWPortList }}%{{ .SSHGWUserList }}%{{ .DoNotExitAfterLoginCommand }}%{{ .DontSpecifyUsername }}%{{ .RemoteEnvironment }}%{{ .PrivateKeyPath }}%{{ .SSHGWPrivateKeyList }}%{{ .SSHBrowserType }}%{{ .FollowSSHPath }}%0%{{ .ProxyType }}%{{ .ProxyHost }}%{{ .ProxyPort }}%{{ .ProxyLogin }}%{{ .AdaptLocales }}%{{ .FileBrowserSCPOverSFTP }}%{{ .FileBrowserProtocol }}%{{ .LocalProxyCommand }}%{{ .SSHProtocolVersion }}%{{ .KeyExchangeAlgos }}%{{ .HostKeyTypes }}%{{ .Ciphers }}%{{ .DisconnectIfAuthSucceedsTrivially }}%{{ .PreferHostKeyAlgorithms }}%{{ .AttemptAuthUsingSSHAgent }}%{{ .AllowAgentForwarding }}#{{ .TerminalFont }}%{{ .FontSize }}%{{ .TerminalFontBold }}%%{{ .AppendWindowsPath }}%{{ .TerminalCharset }}%{{ .Foreground }}%{{ .Background }}%{{ .CursorColor }}%{{ .CursorType }}%{{ .BackspaceSendsCtrlH }}%{{ .LogOutput }}%{{ .LogFolderPath }}%{{ .TerminalType }}%{{ .LockTerminalTitle }}%%{{ .ColorScheme }}%{{ .TerminalRows }}%{{ .TerminalColumns }}%{{ .ForceFixedRowsCols }}%{{ .SyntaxHighlighting }}%{{ .ShowBoldFontAsBrighter }}%{{ .CustomMacroType }}%{{ .CustomMacroText }}%{{ .PasteDelay }}%{{ .FontCharset }}%{{ .FontAntialiasing }}%{{ .FontLigatures }}#{{ .StartSessionIn }}#{{ .Comments }}#{{ .CustomTabColor }}",
      "allowed_options": []
    }
  },
  "options": {
    "Logout": {
      "default_value": "true",
      "section": "Bookmark settings",
      "help": "Determines whether to display a reconnection message when logging out.",
      "options": {
        "false": ";  logout",
        "true": " "
      }
    },
    "IconNumber": {
      "default_value": "109",
      "section": "General",
      "help": "The icon number of the session. The default for SSH is 109."
    },
    "SessionType": {
      "default_value": "0",
      "section": "General",
      "help": "Indicates the session type. For SSH connections, it's always '0'.",
      "options": {
        "SSH": "0"
      }
    },
    "RemoteHost": {
      "default_value": "",
      "required": true,
      "section": "Basic SSH settings",
      "help": "The hostname or IP address of the remote SSH server. This field cannot be empty."
    },
    "Port": {
      "default_value": "22",
      "section": "Basic SSH settings",
      "help": "The port number for the SSH connection. The default SSH port is 22."
    },
    "Username": {
      "default_value": "",
      "section": "Basic SSH settings",
      "help": "The username for the SSH connection."
    },
    "X11Forwarding": {
      "default_value": "Enable",
      "section": "Advanced SSH settings",
      "help": "Enable X11 forwarding.",
      "options": {
        "Enable": "-1",
        "Disable": "0",
        "true": "-1",
        "false": "0"
      }
    },
    "Compression": {
      "default_value": "Enable",
      "section": "Advanced SSH settings",
      "help": "Enable compression.",
      "options": {
        "Enable": "-1",
        "Disable": "0",
        "true": "-1",
        "false": "0"
      }
    },
    "ExecuteCommand": {
      "default_value": "",
      "section": "Advanced SSH settings",
      "help": "Command to execute upon login. ';' in the text field is replaced with '__PTVIRG__' in the setting."
    },
    "SSHGWHostList": {
      "default_value": "",
      "section": "Network settings",
      "help": "List of SSH gateway hosts. Multiple hostnames are separated by '__PIPE__'."
    },
    "SSHGWPortList": {
      "default_value": "",
      "section": "Network settings",
      "help": "List of SSH gateway ports. Multiple ports are separated by '__PIPE__'."
    },
    "SSHGWUserList": {
      "default_value": "",
      "section": "Network settings",
      "help": "List of SSH gateway usernames. Multiple usernames are separated by '__PIPE__'."
    },
    "DoNotExitAfterLoginCommand": {
      "default_value": "false",
      "section": "Advanced SSH settings",
      "help": "Determines whether to exit after the login command ends.",
      "options": {
        "Enable": "-1",
        "Disable": "0",
        "true": "-1",
        "false": "0"
      }
    },
    "DontSpecifyUsername": {
      "default_value": "0",
      "section": "General",
      "help": "Indicates whether to specify a username. '-1' to enable, '0' if a specific username is used.",
      "options": {
        "Enable": "-1",
        "Disable": "0",
        "true": "-1",
        "false": "0"
      }
    },
    "RemoteEnvironment": {
      "default_value": "0",
      "section": "Advanced SSH settings",
